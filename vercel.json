{
  "installCommand": "cd book && npm install",
  "buildCommand": "cd book && npm run build",
  "outputDirectory": "book/build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://rag-backend-production-50e0.up.railway.app/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
