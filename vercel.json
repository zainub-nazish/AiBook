  "outputDirectory": "book/build",
  "framework": "docusaurus-2",
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://rag-backend-production-50e0.up.railway.app/api/$1"
    }
  ]
}
