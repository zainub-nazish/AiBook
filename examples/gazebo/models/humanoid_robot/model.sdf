<?xml version="1.0" ?>
<!--
  Humanoid Robot Model for Digital Twin Simulation
  Module 2: The Digital Twin (Gazebo & Unity)

  This is a simplified humanoid robot for educational purposes.
  For a full model, import from Module 1's URDF using: gz sdf -p model.urdf > model.sdf
-->
<sdf version="1.10">
  <model name="humanoid_robot">
    <static>false</static>

    <!-- Base/Pelvis Link -->
    <link name="pelvis">
      <pose>0 0 0.9 0 0 0</pose>
      <inertial>
        <mass>10.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name="pelvis_visual">
        <geometry>
          <box>
            <size>0.3 0.2 0.15</size>
          </box>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.8 1</ambient>
          <diffuse>0.2 0.2 0.8 1</diffuse>
        </material>
      </visual>
      <collision name="pelvis_collision">
        <geometry>
          <box>
            <size>0.3 0.2 0.15</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Torso Link -->
    <link name="torso">
      <pose>0 0 1.1 0 0 0</pose>
      <inertial>
        <mass>15.0</mass>
        <inertia>
          <ixx>0.2</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.2</iyy>
          <iyz>0</iyz>
          <izz>0.15</izz>
        </inertia>
      </inertial>
      <visual name="torso_visual">
        <geometry>
          <box>
            <size>0.35 0.25 0.4</size>
          </box>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.9 1</ambient>
          <diffuse>0.3 0.3 0.9 1</diffuse>
        </material>
      </visual>
      <collision name="torso_collision">
        <geometry>
          <box>
            <size>0.35 0.25 0.4</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Head Link -->
    <link name="head">
      <pose>0 0 1.45 0 0 0</pose>
      <inertial>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.03</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.03</iyy>
          <iyz>0</iyz>
          <izz>0.03</izz>
        </inertia>
      </inertial>
      <visual name="head_visual">
        <geometry>
          <sphere>
            <radius>0.12</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>0.9 0.8 0.7 1</ambient>
          <diffuse>0.9 0.8 0.7 1</diffuse>
        </material>
      </visual>
      <collision name="head_collision">
        <geometry>
          <sphere>
            <radius>0.12</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

    <!-- Left Upper Arm -->
    <link name="left_upper_arm">
      <pose>0.25 0 1.2 0 0 0</pose>
      <inertial>
        <mass>2.0</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.005</izz>
        </inertia>
      </inertial>
      <visual name="left_upper_arm_visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.4 0.4 0.9 1</ambient>
          <diffuse>0.4 0.4 0.9 1</diffuse>
        </material>
      </visual>
      <collision name="left_upper_arm_collision">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Right Upper Arm -->
    <link name="right_upper_arm">
      <pose>-0.25 0 1.2 0 0 0</pose>
      <inertial>
        <mass>2.0</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.005</izz>
        </inertia>
      </inertial>
      <visual name="right_upper_arm_visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.4 0.4 0.9 1</ambient>
          <diffuse>0.4 0.4 0.9 1</diffuse>
        </material>
      </visual>
      <collision name="right_upper_arm_collision">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Left Upper Leg -->
    <link name="left_upper_leg">
      <pose>0.1 0 0.6 0 0 0</pose>
      <inertial>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.05</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="left_upper_leg_visual">
        <geometry>
          <cylinder>
            <radius>0.06</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.7 1</ambient>
          <diffuse>0.2 0.2 0.7 1</diffuse>
        </material>
      </visual>
      <collision name="left_upper_leg_collision">
        <geometry>
          <cylinder>
            <radius>0.06</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Right Upper Leg -->
    <link name="right_upper_leg">
      <pose>-0.1 0 0.6 0 0 0</pose>
      <inertial>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.05</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="right_upper_leg_visual">
        <geometry>
          <cylinder>
            <radius>0.06</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.7 1</ambient>
          <diffuse>0.2 0.2 0.7 1</diffuse>
        </material>
      </visual>
      <collision name="right_upper_leg_collision">
        <geometry>
          <cylinder>
            <radius>0.06</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Joints -->
    <joint name="pelvis_to_torso" type="fixed">
      <parent>pelvis</parent>
      <child>torso</child>
    </joint>

    <joint name="torso_to_head" type="revolute">
      <parent>torso</parent>
      <child>head</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

    <joint name="left_shoulder" type="revolute">
      <parent>torso</parent>
      <child>left_upper_arm</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-3.14</lower>
          <upper>3.14</upper>
        </limit>
      </axis>
    </joint>

    <joint name="right_shoulder" type="revolute">
      <parent>torso</parent>
      <child>right_upper_arm</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-3.14</lower>
          <upper>3.14</upper>
        </limit>
      </axis>
    </joint>

    <joint name="left_hip" type="revolute">
      <parent>pelvis</parent>
      <child>left_upper_leg</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

    <joint name="right_hip" type="revolute">
      <parent>pelvis</parent>
      <child>right_upper_leg</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

  </model>
</sdf>
